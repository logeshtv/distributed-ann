# Environment Variables Template
# Copy this file to .env and fill in your API keys

# Alpaca API (US Stocks) - https://alpaca.markets/
# Note: For historical data only, you can leave SECRET_KEY empty
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=
ALPACA_BASE_URL=https://paper-api.alpaca.markets

# Binance API (Crypto) - https://www.binance.com/en/binance-api
# Note: Historical data can be fetched without API keys
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here

# Database
DATABASE_URL=sqlite:///data_storage/trading.db

# Trading Configuration
INITIAL_CAPITAL=100000
PAPER_TRADING=true
